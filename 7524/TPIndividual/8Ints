local Ints in
   local L in
      local A in
	 local B in
	    local C in
	       fun lazy {Ints N}
		  {Show N}
		  N|{Ints N+1}
	       end
	       {Show 'Comienzo'}
	       L = {Ints 1}
	       {Show 'Asignacion de L'}
	       A = L.2.2.1
	       {Show 'Asignacion de A usando L'}
	       B = L.1
	       {Show 'Asignacion de B usando L'}
	       C = A+B
	       {Show 'Asignacion de C'}
	       {Show C}
	    end
	 end
      end
   end
end